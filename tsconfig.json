{
  "extends": "astro/tsconfigs/strict",
  "include": ["**/*"],
  "exclude": ["dist/**/*"],
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "paths": {
      "!root/*": ["*"],
      "!assets/*": ["src/assets/*"],
      "!layouts/*": ["src/layouts/*"],
      "!components/*": ["src/components/*"],
      "!solid/*": ["src/components/solid/*"]
    }
  }
}
